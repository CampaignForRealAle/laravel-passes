{
    "name": "chiiya/laravel-passes",
    "description": "This is my package laravel-passes",
    "keywords": [
        "chiiya",
        "laravel",
        "laravel-passes"
    ],
    "homepage": "https://github.com/chiiya/laravel-passes",
    "license": "MIT",
    "authors": [
        {
            "name": "Elisha Witte",
            "email": "github@chiiya.moe",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^9.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.0",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Chiiya\\LaravelPasses\\": "src",
            "Chiiya\\LaravelPasses\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chiiya\\LaravelPasses\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chiiya\\LaravelPasses\\LaravelPassesServiceProvider"
            ],
            "aliases": {
                "LaravelPasses": "Chiiya\\LaravelPasses\\Facades\\LaravelPasses"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
